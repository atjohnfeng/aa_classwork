<h2>Log In</h2>

<form action="<%= session_url %>" method="POST">

<label>Username 
    <input type="text" name="user[user_name]" value="<%= @user.user_name%>">
</label>
<br>
<label>Password 
    <input type="password" name="user[password]" value="">
</label>
<br>
<input type="submit" value="Log In">

</form>